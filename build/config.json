{
  "productName": "VK Desktop",
  "appId": "ru.danyadev.vkdesktop",
  "artifactName": "VK-Desktop-${os}-${arch}.${ext}",
  "electronVersion": "7.3.0",
  "npmRebuild": false,
  "directories": {
    "output": "out"
  },
  "files": ["dist/**", "menu.js", "index.js"],
  "win": {
    "target": [{
      "target": "dir",
      "arch": ["x64", "ia32"]
    }]
  },
  "linux": {
    "target": [{
      "target": "zip",
      "arch": ["x64", "ia32"]
    }]
  },
  "mac": {
    "category": "public.app-category.social-networking",
    "gatekeeperAssess": true,
    "target": "dmg"
  }
}
